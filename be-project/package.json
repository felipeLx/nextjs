{
  "private": true,
  "author": "Felipe Lisboa - felipealisboa@outlook.com",
  "description": "full stack aplicacion to ecommerce",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@prisma/client": "^3.4.1",
    "@react-spring/web": "^9.3.0",
    "@vercel/fetch": "^6.1.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "email-validator": "^2.0.4",
    "fs": "0.0.1-security",
    "immutability-helper": "^3.1.1",
    "js-cookies": "^1.0.4",
    "lodash.debounce": "^4.0.8",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^12.0.3",
    "next-seo": "^4.28.1",
    "next-themes": "0.0.15",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-fast-marquee": "^1.2.1",
    "react-merge-refs": "^1.1.0",
    "react-use-measure": "^2.0.4",
    "stripe": "^8.186.1",
    "swell-js": "^3.12.0",
    "swr": "^1.0.1",
    "tabbable": "^5.2.1",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/body-scroll-lock": "^3.1.0",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^3.0.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "autoprefixer": "^10.4.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.2.0",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.5",
    "prettier": "^2.4.1",
    "prisma": "^3.4.1",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
